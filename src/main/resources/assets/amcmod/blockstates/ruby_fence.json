{
    "multipart": [
        {   "apply": { "model": "amcmod:block/ruby_fence" }},
        {   "when": { "north": "true" },
            "apply": { "model": "amcmod:block/ruby_fence", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "amcmod:block/ruby_fence", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "amcmod:block/ruby_fence", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "amcmod:block/ruby_fence", "y": 270, "uvlock": true }
        }
    ]
}
